{
  "name": "@gbraver-burst-network/backend-ecs",
  "description": "create backend ecs.",
  "version": "1.9.2",
  "bin": {
    "backend-ecs": "bin/backend-ecs.js"
  },
  "dependencies": {
    "@types/ramda": "^0.29.2",
    "aws-cdk-lib": "^2.83.1",
    "constructs": "^10.2.52",
    "dotenv": "^16.1.4",
    "ramda": "^0.29.0",
    "source-map-support": "^0.5.21"
  },
  "devDependencies": {
    "@types/jest": "^29.5.2",
    "@types/node": "20.3.1",
    "@types/uuid": "^9.0.2",
    "@typescript-eslint/eslint-plugin": "^5.59.11",
    "@typescript-eslint/parser": "^5.59.11",
    "aws-cdk": "2.83.1",
    "eslint": "^8.42.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "jest": "^29.5.0",
    "madge": "^6.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "2.8.8",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "typescript": "~5.1.3"
  },
  "private": true,
  "scripts": {
    "build": "tsc",
    "cdk": "cdk",
    "clean": "tsc --build --clean",
    "code-format": "run-s code-format:lib code-format:bin code-format:test",
    "code-format:bin": "prettier --write bin",
    "code-format:lib": "prettier --write lib",
    "code-format:test": "prettier --write bin",
    "detect-circular": "run-s detect-circular:bin detect-circular:lib detect-circular:test",
    "detect-circular:bin": "madge --extensions ts -c bin",
    "detect-circular:lib": "madge --extensions ts -c lib",
    "detect-circular:test": "madge --extensions ts -c test",
    "lint": "run-s lint:lib lint:bin lint:test",
    "lint-fix": "run-s lint-fix:lib lint-fix:bin lint-fix:test",
    "lint-fix:bin": "eslint --fix --max-warnings 0 bin",
    "lint-fix:lib": "eslint --fix --max-warnings 0 lib",
    "lint-fix:test": "eslint --fix --max-warnings 0 test",
    "lint:bin": "eslint --max-warnings 0 bin",
    "lint:lib": "eslint --max-warnings 0 lib",
    "lint:test": "eslint --max-warnings 0 test",
    "test": "jest",
    "watch": "tsc -w"
  }
}
