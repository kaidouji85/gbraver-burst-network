{
  "name": "@gbraver-burst-network/backend-ecs",
  "version": "1.5.9",
  "private": true,
  "bin": {
    "backend-ecs": "bin/backend-ecs.js"
  },
  "scripts": {
    "build": "run-s lint transpile test",
    "transpile": "tsc",
    "lint": "run-s lint:lib lint:bin lint:test",
    "lint:lib": "eslint lib",
    "lint:bin": "eslint bin",
    "lint:test": "eslint test",
    "lint-fix": "run-s lint-fix:lib lint-fix:bin lint-fix:test",
    "lint-fix:lib": "eslint --fix lib",
    "lint-fix:bin": "eslint --fix bin",
    "lint-fix:test": "eslint --fix test",
    "code-format": "run-s code-format:lib code-format:bin code-format:test",
    "code-format:lib": "prettier --write lib",
    "code-format:bin": "prettier --write bin",
    "code-format:test": "prettier --write bin",
    "watch": "tsc -w",
    "test": "jest",
    "cdk": "cdk"
  },
  "devDependencies": {
    "@types/jest": "^29.2.0",
    "@types/node": "18.11.7",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.41.0",
    "@typescript-eslint/parser": "^5.41.0",
    "aws-cdk": "2.49.0",
    "eslint": "^8.26.0",
    "eslint-plugin-simple-import-sort": "^8.0.0",
    "jest": "^29.2.2",
    "npm-run-all": "^4.1.5",
    "prettier": "2.7.1",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typescript": "~4.8.4"
  },
  "dependencies": {
    "aws-cdk-lib": "^2.49.0",
    "constructs": "^10.1.143",
    "dotenv": "^16.0.3",
    "source-map-support": "^0.5.21",
    "uuid": "^9.0.0"
  }
}
