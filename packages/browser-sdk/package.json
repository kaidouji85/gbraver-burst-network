{
  "name": "@gbraver-burst-network/browser-sdk",
  "version": "1.0.0",
  "description": "gbraver burst browser sdk",
  "main": "lib/index.js",
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "run-s clean type-check lint test transpile copy-type-definition",
    "type-check": "flow",
    "lint": "eslint src",
    "lint:test": "eslint test",
    "test": "ava -T 600000",
    "transpile": "babel src -d lib/",
    "copy-type-definition": "flow-copy-source src lib",
    "clean": "rimraf lib"
  },
  "author": "Y.Takeuchi",
  "license": "ISC",
  "dependencies": {
    "@auth0/auth0-spa-js": "^1.19.1",
    "@gbraver-burst-network/browser-core": "^1.0.0",
    "gbraver-burst-core": "^1.10.0",
    "rxjs": "^7.4.0"
  },
  "devDependencies": {
    "@ava/babel": "^2.0.0",
    "@babel/cli": "^7.15.7",
    "@babel/core": "^7.15.8",
    "@babel/preset-env": "^7.15.8",
    "@babel/preset-flow": "^7.14.5",
    "@babel/register": "^7.15.3",
    "ava": "^3.15.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "eslint": "^7.32.0",
    "eslint-plugin-flowtype": "^6.1.0",
    "flow-bin": "^0.162.0",
    "flow-copy-source": "^2.0.9",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "webpack": "^5.58.2",
    "webpack-cli": "^4.9.0"
  },
  "ava": {
    "babel": {
      "compileEnhancements": false
    },
    "files": [
      "test/src/**/*.js"
    ],
    "require": [
      "@babel/register"
    ]
  }
}
