    "code-format:test": "prettier --write bin",
    "lint": "run-s lint:lib lint:bin lint:test",
    "lint-fix": "run-s lint-fix:lib lint-fix:bin lint-fix:test",
    "lint-fix:bin": "eslint --fix bin",
    "lint-fix:lib": "eslint --fix lib",
    "lint-fix:test": "eslint --fix test",
    "lint:bin": "eslint bin",
    "lint:lib": "eslint lib",
    "lint:test": "eslint test",
    "test": "jest",
    "transpile": "tsc",
    "watch": "tsc -w"
  }
