{
  "name": "@gbraver-burst-network/aws-vpc",
  "description": "gbraver burst backend vpc.",
  "version": "1.6.3",
  "bin": {
    "aws-vpc": "bin/aws-vpc.js"
  },
  "dependencies": {
    "aws-cdk-lib": "^2.61.1",
    "constructs": "^10.1.226",
    "dotenv": "^16.0.3",
    "source-map-support": "^0.5.21"
  },
  "devDependencies": {
    "@types/jest": "^29.2.6",
    "@types/node": "18.11.18",
    "@typescript-eslint/eslint-plugin": "^5.48.2",
    "@typescript-eslint/parser": "^5.48.2",
    "aws-cdk": "2.61.1",
    "eslint": "^8.32.0",
    "eslint-plugin-simple-import-sort": "^9.0.0",
    "jest": "^29.3.1",
    "npm-run-all": "^4.1.5",
    "prettier": "2.8.3",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "typescript": "~4.9.4"
  },
  "private": true,
  "scripts": {
    "build": "run-s lint transpile test",
    "cdk": "cdk",
    "code-format": "run-s code-format:lib code-format:bin code-format:test",
