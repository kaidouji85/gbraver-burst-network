{
  "name": "@gbraver-burst-network/serverless-stub",
  "description": "gbraver burst network stub.",
  "version": "1.9.2",
  "author": "Y.Takeuchi",
  "dependencies": {
    "@gbraver-burst-network/browser-sdk": "1.9.2",
    "dotenv": "^16.3.1",
    "gbraver-burst-core": "^1.18.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.60.0",
    "@typescript-eslint/parser": "^5.60.0",
    "codehawk-cli": "^10.1.0",
    "eslint": "^8.43.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "html-webpack-plugin": "^5.5.3",
    "http-server": "^14.1.1",
    "madge": "^6.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "2.8.8",
    "rimraf": "^5.0.1",
    "ts-loader": "^9.4.3",
    "typescript": "^5.1.3",
    "webpack": "^5.87.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  },
  "license": "ISC",
  "main": "lib/index.js",
  "private": true,
  "scripts": {
    "build": "run-s clean transpile",
    "clean": "rimraf build",
    "code-analysis": "codehawk src",
    "code-format": "prettier --write src",
    "detect-circular": "madge --extensions ts -c src",
    "dev-server": "webpack serve",
    "lint": "eslint --max-warnings 0 src",
    "lint-fix": "eslint --fix --max-warnings 0 src",
    "serve": "http-server build -p 8080 -c-1",
    "start": "run-s clean dev-server",
    "start:production": "run-s build serve",
    "transpile": "webpack --mode=production",
    "type-check": "tsc --noEmit"
  }
}
