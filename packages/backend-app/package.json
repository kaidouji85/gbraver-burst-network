{
  "name": "@gbraver-burst-network/backend-app",
  "description": "gbraver burst backend app",
  "version": "1.6.3",
  "author": "Y.Takeuchi",
  "dependencies": {
    "auth0": "^3.1.1",
    "aws-sdk": "^2.1299.0",
    "gbraver-burst-core": "^1.16.4",
    "jsonwebtoken": "^9.0.0",
    "jwks-rsa": "^3.0.1",
    "ramda": "^0.28.0",
    "rimraf": "^4.1.1",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/auth0": "^2.35.9",
    "@types/jest": "^29.2.6",
    "@types/ramda": "^0.28.21",
    "@types/uuid": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^5.48.2",
    "@typescript-eslint/parser": "^5.48.2",
    "codehawk-cli": "^10.1.0",
    "eslint": "^8.32.0",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-simple-import-sort": "^9.0.0",
    "jest": "^29.3.1",
    "npm-run-all": "^4.1.5",
    "prettier": "2.8.3",
    "serverless": "^3.26.0",
    "serverless-webpack": "^5.11.0",
    "superagent-proxy": "^3.0.0",
    "ts-jest": "^29.0.5",
    "ts-loader": "^9.4.2",
    "typescript": "^4.9.4",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1"
  },
  "license": "ISC",
  "private": true,
  "scripts": {
    "all-check": "run-s type-check lint lint:test test",
    "build": "run-s clean all-check transpile:sls transpile:match-make",
    "build:match-make": "run-s clean:webpack all-check transpile:match-make",
    "build:sls": "run-s clean:sls all-check transpile:sls",
    "clean": "run-s clean:sls clean:webpack",
    "clean:sls": "rimraf .serverless",
    "clean:webpack": "rimraf .webpack",
    "code-analysis": "codehawk src",
    "code-format": "prettier --write src",
    "code-format:test": "prettier --write test",
    "lint": "eslint src",
    "lint-fix": "eslint --fix src",
    "lint-fix:test": "eslint --fix test",
    "lint:test": "eslint test",
    "serve:match-make": "node .webpack/match-making-polling.js",
    "start:match-make": "run-s build:match-make serve:match-make",
    "test": "jest",
    "transpile:match-make": "webpack --config matchMake.webpack.config.js",
    "transpile:sls": "sls package",
    "type-check": "tsc --noEmit"
  }
}
