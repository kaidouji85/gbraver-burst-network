{
  "name": "@gbraver-burst-network/backend-app",
  "description": "gbraver burst backend app",
  "version": "1.9.0",
  "author": "Y.Takeuchi",
  "dependencies": {
    "@aws-sdk/client-apigatewaymanagementapi": "^3.306.0",
    "@aws-sdk/client-dynamodb": "^3.306.0",
    "@aws-sdk/lib-dynamodb": "^3.306.0",
    "@aws-sdk/signature-v4-crt": "^3.306.0",
    "auth0": "^3.3.0",
    "gbraver-burst-core": "^1.16.4",
    "jsonwebtoken": "^9.0.0",
    "jwks-rsa": "^3.0.1",
    "nanoid": "^4.0.2",
    "ramda": "^0.29.0",
    "rimraf": "^4.4.1",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/auth0": "^3.3.1",
    "@types/jest": "^29.5.0",
    "@types/ramda": "^0.28.23",
    "@types/uuid": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "^5.57.0",
    "@typescript-eslint/parser": "^5.57.0",
    "codehawk-cli": "^10.1.0",
    "eslint": "^8.37.0",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "jest": "^29.5.0",
    "npm-run-all": "^4.1.5",
    "prettier": "2.8.7",
    "serverless": "^3.29.0",
    "serverless-webpack": "^5.11.0",
    "superagent-proxy": "^3.0.0",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.2",
    "typescript": "^5.0.3",
    "webpack": "^5.77.0",
    "webpack-cli": "^5.0.1"
  },
  "license": "ISC",
  "private": true,
  "scripts": {
    "build": "run-s clean transpile",
    "build:match-make": "run-s clean:webpack transpile:match-make",
    "build:sls": "run-s clean:sls transpile:sls",
    "clean": "run-s clean:sls clean:webpack",
    "clean:sls": "rimraf .serverless",
    "clean:webpack": "rimraf .webpack",
    "code-analysis": "codehawk src",
    "code-format": "run-s code-format:src code-format:test",
    "code-format:src": "prettier --write src",
    "code-format:test": "prettier --write test",
    "lint": "run-s lint:src lint:test",
    "lint-fix": "run-s lint-fix:src lint-fix:test",
    "lint-fix:src": "eslint --fix src",
    "lint-fix:test": "eslint --fix test",
    "lint:src": "eslint src",
    "lint:test": "eslint test",
    "serve:match-make": "node .webpack/match-making-polling.js",
    "start:match-make": "run-s build:match-make serve:match-make",
    "test": "jest",
    "transpile": "run-s transpile:sls transpile:match-make",
    "transpile:match-make": "webpack --config matchMake.webpack.config.js",
    "transpile:sls": "sls package",
    "type-check": "tsc --noEmit"
  }
}
