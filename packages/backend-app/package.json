{
  "name": "@gbraver-burst-network/backend-app",
  "description": "gbraver burst backend app",
  "version": "1.9.2",
  "author": "Y.Takeuchi",
  "dependencies": {
    "@aws-sdk/client-apigatewaymanagementapi": "^3.354.0",
    "@aws-sdk/client-dynamodb": "^3.354.0",
    "@aws-sdk/lib-dynamodb": "^3.354.0",
    "@aws-sdk/signature-v4-crt": "^3.354.0",
    "auth0": "^3.4.0",
    "gbraver-burst-core": "^1.18.0",
    "jsonwebtoken": "^9.0.0",
    "jwks-rsa": "^3.0.1",
    "nanoid": "^4.0.2",
    "ramda": "^0.29.0",
    "rimraf": "^5.0.1",
    "uuid": "^9.0.0",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@types/auth0": "^3.3.3",
    "@types/jest": "^29.5.2",
    "@types/ramda": "^0.29.2",
    "@types/uuid": "^9.0.2",
    "@typescript-eslint/eslint-plugin": "^5.60.0",
    "@typescript-eslint/parser": "^5.60.0",
    "codehawk-cli": "^10.1.0",
    "eslint": "^8.43.0",
    "eslint-plugin-jest": "^27.2.2",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "jest": "^29.5.0",
    "madge": "^6.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "2.8.8",
    "serverless": "^3.32.2",
    "serverless-webpack": "^5.11.0",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "typescript": "^5.1.3",
    "webpack": "^5.87.0",
    "webpack-cli": "^5.1.4"
  },
  "license": "ISC",
  "private": true,
  "scripts": {
    "build": "run-s clean transpile",
    "build:match-make": "run-s clean:webpack transpile:match-make",
    "build:sls": "run-s clean:sls transpile:sls",
    "clean": "run-s clean:*",
    "clean:sls": "rimraf .serverless",
    "clean:webpack": "rimraf .webpack",
    "code-analysis": "codehawk src",
    "code-format": "run-s code-format:*",
    "code-format:src": "prettier --write src",
    "code-format:test": "prettier --write test",
    "detect-circular": "run-s detect-circular:*",
    "detect-circular:src": "madge --extensions ts -c src",
    "detect-circular:test": "madge --extensions ts -c test",
    "lint": "run-s lint:*",
    "lint-fix": "run-s lint-fix:*",
    "lint-fix:src": "eslint --fix --max-warnings 0 src",
    "lint-fix:test": "eslint --fix --max-warnings 0 test",
    "lint:src": "eslint --max-warnings 0 src",
    "lint:test": "eslint --max-warnings 0 test",
    "serve:match-make": "node .webpack/match-making-polling.js",
    "start:match-make": "run-s build:match-make serve:match-make",
    "test": "jest",
    "transpile": "run-s transpile:*",
    "transpile:match-make": "webpack --config matchMake.webpack.config.js",
    "transpile:sls": "sls package",
    "type-check": "tsc --noEmit"
  }
}
