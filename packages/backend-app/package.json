{
  "name": "@gbraver-burst-network/backend-app",
  "version": "1.5.4",
  "description": "gbraver burst backend app",
  "private": true,
  "scripts": {
    "start:match-make": "run-s build:match-make serve:match-make",
    "clean": "run-s clean:sls clean:webpack",
    "clean:sls": "rimraf .serverless",
    "clean:webpack": "rimraf .webpack",
    "build": "run-s clean all-check transpile:sls transpile:match-make",
    "build:sls": "run-s clean:sls all-check transpile:sls",
    "build:match-make": "run-s clean:webpack all-check transpile:match-make",
    "all-check": "run-s type-check lint lint:test test",
    "type-check": "flow",
    "lint": "eslint src",
    "lint:test": "eslint test",
    "test": "jest",
    "transpile:sls": "sls package",
    "transpile:match-make": "webpack --config matchMake.webpack.config.js",
    "serve:match-make": "node .webpack/match-making-polling.js"
  },
  "dependencies": {
    "auth0": "^2.40.0",
    "aws-sdk": "^2.1073.0",
    "gbraver-burst-core": "^1.11.3",
    "jsonwebtoken": "^8.5.1",
    "jwks-rsa": "^2.0.5",
    "ramda": "^0.28.0",
    "rimraf": "^3.0.2",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.17.2",
    "@babel/eslint-parser": "^7.17.0",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-flow": "^7.16.7",
    "@babel/register": "^7.17.0",
    "babel-loader": "^8.2.3",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "eslint": "^8.9.0",
    "eslint-plugin-flowtype": "^8.0.3",
    "eslint-plugin-jest": "^26.1.0",
    "flow-bin": "^0.171.0",
    "flow-typed": "^3.6.1",
    "jest": "^27.5.1",
    "npm-run-all": "^4.1.5",
    "serverless": "^3.2.0",
    "serverless-webpack": "^5.6.1",
    "superagent-proxy": "^3.0.0",
    "webpack": "^5.68.0",
    "webpack-cli": "^4.9.2"
  },
  "author": "Y.Takeuchi",
  "license": "ISC"
}
