{
  "name": "@gbraver-burst-network/serverless",
  "version": "0.6.1",
  "description": "gbraver burst serverless",
  "private": true,
  "scripts": {
    "start:match-make": "run-s build:mach-make serve:match-make",
    "deploy": "run-s type-check lint lint:test test upload",
    "build": "run-s type-check lint lint:test test package",
    "build:mach-make": "run-s type-check lint lint:test test transpile:mach-make",
    "type-check": "flow",
    "lint": "eslint src",
    "lint:test": "eslint test",
    "test": "ava -T 600000",
    "package": "sls package",
    "transpile:mach-make": "webpack --config webpack.match.make.js",
    "upload": "sls deploy",
    "serve:match-make": "node .webpack/match-making-polling.js.js"
  },
  "dependencies": {
    "aws-sdk": "^2.987.0",
    "gbraver-burst-core": "^1.10.0",
    "jsonwebtoken": "^8.5.1",
    "jwks-rsa": "^2.0.4",
    "ramda": "^0.27.1",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@ava/babel": "^2.0.0",
    "@babel/core": "^7.15.5",
    "@babel/preset-env": "^7.15.6",
    "@babel/preset-flow": "^7.14.5",
    "@babel/register": "^7.15.3",
    "ava": "^3.15.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "eslint": "^7.32.0",
    "eslint-plugin-flowtype": "^5.9.2",
    "flow-bin": "^0.159.0",
    "npm-run-all": "^4.1.5",
    "serverless": "^2.57.0",
    "serverless-webpack": "^5.5.4",
    "webpack": "5.52.1",
    "webpack-cli": "^4.8.0"
  },
  "author": "The serverless webpack authors (https://github.com/elastic-coders/serverless-webpack)",
  "license": "ISC",
  "ava": {
    "babel": {
      "compileEnhancements": false
    },
    "files": [
      "test/src/**/*.js"
    ],
    "require": [
      "@babel/register"
    ]
  }
}
