    "test": "ava -T 600000",
    "package": "sls package",
    "upload": "sls deploy"
  },
  "dependencies": {
    "aws-sdk": "^2.978.0",
    "gbraver-burst-core": "^1.9.0",
    "jsonwebtoken": "^8.5.1",
    "jwks-rsa": "^2.0.4",
    "ramda": "^0.27.1",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@ava/babel": "^2.0.0",
    "@babel/core": "^7.15.0",
    "@babel/preset-env": "^7.15.0",
    "@babel/preset-flow": "^7.14.5",
    "@babel/register": "^7.15.3",
    "ava": "^3.15.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "eslint": "^7.32.0",
    "eslint-plugin-flowtype": "^5.9.1",
    "flow-bin": "^0.158.0",
    "npm-run-all": "^4.1.5",
    "serverless": "^2.56.0",
    "serverless-webpack": "^5.5.2",
    "webpack": "^5.51.1"
  },
  "author": "The serverless webpack authors (https://github.com/elastic-coders/serverless-webpack)",
  "license": "ISC",
  "ava": {
    "babel": {
      "compileEnhancements": false
